# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/scanneo_router?sslmode=require

# OpenRouteService API (optional, for routing)
ORS_API_KEY=your-ors-api-key

# Redis Configuration (optional, for job queue)
UPSTASH_REDIS_REST_URL=https://your-redis-instance.upstash.io
UPSTASH_REDIS_REST_TOKEN=your-redis-token

# Overpass API (OpenStreetMap)
OVERPASS_URL=https://overpass-api.de/api/interpreter

# Worker Settings
POLL_INTERVAL=30  # How often to check for jobs (seconds)
JOB_TIMEOUT=3600  # Maximum job processing time (seconds)
MAX_RETRIES=3

# Environment
ENVIRONMENT=development
LOG_LEVEL=INFO